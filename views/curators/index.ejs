<!-- Header -->
<h1>Curators Home page</h1>
<!-- body -->
      <h2>List of Curators</h2>
      <% if(!curators.length) { %>
        <p>There are no Curators available yet.</p>
      <% } %>

      <ul>
        <% curators.forEach(curator => { %>
        <li>
          <a href="/curators/<%= curator._id %>"> <%= curator.name %> </a>|
          <a href="<%= curator.url %>">Gallery</a>
        </li>
        <% }) %>
      </ul>


